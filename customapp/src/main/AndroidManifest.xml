<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.siberiadante.custom">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ui.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemeA"
        tools:replace="android:icon,android:theme,android:label">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.SplashActivity" />
        <activity android:name=".ui.activity.Main2Activity" />
        <activity android:name=".ui.activity.DownloadActivity" />
        <activity android:name=".ui.activity.user.RegisterActivity" />
        <activity android:name=".ui.activity.user.LoginActivity"></activity>
    </application>

</manifest>